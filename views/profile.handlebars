<div class="ourteam">
    <div class="container">
        <h6>Profile</h6>
      <div class="col-xs-4">
          <div>
              <img src="./../img/p3.jpg" class="img-responsive" alt="">
          </div>
        <div class="margin-horizontal-16">
            <h4>{{profile.name}}</h4>
        </div>
      </div>
        <div class="col-xs-8">
            <!-- Tip -->
            <div class="new-user-avatar-cta mb-4 py-2 px-3">
                <p class="mt-3">
                    My role is
                    <strong> {{profile.role}}</strong>
                </p>
            </div>
          <!-- Tabs -->
            <ul class="nav nav-tabs">
                <li class="active"><a href="#tab1" data-toggle="tab" aria-expanded="true">Recommended Cases</a></li>
                <!--<li><a href="#tab2" data-toggle="tab">Learning List</a></li>-->
            </ul>
            <div class="tab-content" id="tabs">
                <div class="tab-pane about active" id="tab1">
                    <ul>
                        {{#each case}}
                            <li><a href="/case?id={{id}}"><span></span>{{name}} </a></li>
                        {{/each}}
                    </ul>
                </div>
            </div>

        </div>
    </div>
</div>
